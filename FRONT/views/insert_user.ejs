<%-include('partials/head') %> 
<body id="fundo">
    <%-include('partials/navbar') %> 
    <div class="card" id="telaLogin">

        <div class="card-body">
          <form class="row g-3" action="/" method="get">
            <div class="col-md-12">
              <label for="validationDefault01" class="form-label">Nome</label>
              <input type="text" class="form-control" id="validationDefault01" required placeholder="Digite Seu Nome">
            </div>
            <div class="col-md-4">
              <label for="validationDefault02" class="form-label">CPF</label>
              <input type="number" class="form-control" id="validationDefault02" required placeholder="Digite Seu CPF">
            </div>
            <div class="col-md-4">
              <label for="validationDefault03" class="form-label">Data de Nascimento</label>
              <div class="input-group">
                <input type="date" class="form-control" id="validationDefault03"  aria-describedby="inputGroupPrepend2" required>
              </div>
            </div>
            <div class="col-md-4">
              <label for="validationDefault05" class="form-label">Telefone</label>
              <input type="tel" class="form-control" id="validationDefault06" placeholder="Digite seu Telefone">
            </div>
            <div class="col-md-6">
              <label for="validationDefault04" class="form-label">Email</label>
              <input type="email" class="form-control" id="validationDefault04" placeholder="Digite seu E-mail">
            </div>
            <div class="col-md-6">
              <label for="validationDefault06" class="form-label">Senha
                
              </label>
              <input type="password" class="form-control" id="validationDefault06" required>
              
            </div>
            <div class="col-12">
              <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="invalidCheck2" required>
                <label class="form-check-label" for="invalidCheck2">
                  Aceito os termos de uso e condições
                </label>
              </div>
            </div>
            <div class="col-12">
              <button class="btn btn-primary" type="submit">Cadastrar</button>
            </div>
          </form>
      </div>
</body>
</html>